{
  "manifest_version": 3,
  "name": "Bookmark Daily",
  "short_name": "Bookmarked",
  "version": "1.0.0",
  "description": "Keep your bookmarked pages clean from unused Bookmarks!",
  "icons": {
    "16": "images/icon.jpeg",
    "32": "images/icon.jpeg",
    "48": "images/icon.jpeg",
    "128": "images/icon.jpeg",
    "192": "images/icon.jpeg"
  },
  "permissions": ["bookmarks"],
  "action": {
    "default_popup": "extension.html"
  }
}
